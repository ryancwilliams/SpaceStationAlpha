<?xml version="1.0"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            elementFormDefault="qualified" targetNamespace="http://github.com/ryancwilliams/SpaceStationAlpha" xmlns:tns="http://github.com/ryancwilliams/SpaceStationAlpha">
    <xsd:element name="spriteSheet">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sprite" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="name" type="xsd:string" minOccurs="1" maxOccurs="1" nillable="false"/>
                            <xsd:element name="xCoord" type="xsd:integer" minOccurs="1" nillable="false" maxOccurs="1"></xsd:element>
                            <xsd:element name="yCoord" type="xsd:integer" minOccurs="1" maxOccurs="1" nillable="false"></xsd:element>
                            <xsd:element name="width" type="xsd:integer" minOccurs="1" maxOccurs="1" nillable="false"></xsd:element>
                            <xsd:element name="height" type="xsd:integer" minOccurs="1" maxOccurs="1" nillable="false"></xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
