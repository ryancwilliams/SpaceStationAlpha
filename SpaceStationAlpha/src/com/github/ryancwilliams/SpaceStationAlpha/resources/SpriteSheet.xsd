<?xml version="1.0"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            elementFormDefault="qualified" targetNamespace="http://github.com/ryancwilliams/SpaceStationAlpha" xmlns:tns="http://github.com/ryancwilliams/SpaceStationAlpha">
    <xsd:element name="spriteSheet">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sprite" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:attribute name="name" type="xsd:string"/>
                            <xsd:attribute name="xCoord" type="xsd:integer"/>
                            <xsd:attribute name="yCoord" type="xsd:integer"/>
                            <xsd:attribute name="width" type="xsd:integer"/>
                            <xsd:attribute name="height" type="xsd:integer"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
